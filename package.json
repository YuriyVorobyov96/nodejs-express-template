{
  "name": "express-template",
  "version": "1.0.0",
  "description": "Express application template for A2SEVEN",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/src/main.js",
    "dev": "nodemon",
    "inspect": "nodemon -e ts,json --exec node --inspect=localhost:9229 -r ts-node/register src/main.ts",
    "test": "jest",
    "lint": "eslint ./src/**",
    "lint:fix": "eslint ./src/** --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migrate:up": "npm run typeorm -- migration:run -d src/database/data-source.ts",
    "migrate:down": "npm run typeorm -- migration:revert -d src/database/data-source.ts",
    "migrate:generate": "bash scripts/migration-generate.sh",
    "migrate:create": "bash scripts/migration-create.sh"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.0.1",
    "body-parser": "1.20.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "config": "3.3.8",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "http-status-codes": "2.2.0",
    "inversify": "6.0.1",
    "jsonwebtoken": "8.5.1",
    "pg": "8.8.0",
    "reflect-metadata": "0.1.13",
    "tslog": "3.3.4",
    "typeorm": "0.3.10",
    "typeorm-naming-strategies": "4.1.0"
  },
  "devDependencies": {
    "@a2seven/eslint-config": "^1.0.4",
    "@types/bcrypt": "5.0.0",
    "@types/config": "3.3.0",
    "@types/express": "4.17.14",
    "@types/jest": "29.0.3",
    "@types/jsonwebtoken": "8.5.9",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-max-params-no-constructor": "^0.0.4",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest": "29.0.3",
    "nodemon": "2.0.20",
    "prettier": "^2.7.1",
    "ts-jest": "29.0.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.3"
  }
}
